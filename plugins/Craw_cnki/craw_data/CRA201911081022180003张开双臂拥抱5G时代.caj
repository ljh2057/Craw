    <script>
        var mallReqUrl = "//bianke.cnki.net/adpromotion/api/AdContentApi/WriteAd";
		$(function(){		
		    try
			{			
				$.ajax({
				    url: mallReqUrl,
                    type: "GET",
                    dataType: "jsonp", 
                    success: function (data) {
                        try {
							var resdata=data;
							if (typeof resdata == "string")
								resdata = JSON.parse(resdata);			
								
							if (resdata.state === "success"){
							    $("#loginMain").css("background-image","url("+ resdata.data[0].img+")");
							    $("#mallLink").attr("href", resdata.data[0].link);
							}
							//console.log(resdata);
						}
						catch (e) {
							console.log(e);
						}
                    },
				 error: function (XMLHttpRequest, textStatus, errorThrown) {
				       console.log( XMLHttpRequest.status + XMLHttpRequest.statusText + " " + XMLHttpRequest.readyState + " " + textStatus);
				    }
                });
			}
			catch (e) {
				console.log(e);
			}
		});        
